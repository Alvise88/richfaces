<?xml version="1.0" encoding="UTF-8"?>

<cdk:root xmlns="http://jboss.org/schema/richfaces/cdk/xhtml-el" xmlns:cdk="http://jboss.org/schema/richfaces/cdk/core"
          xmlns:cc="http://jboss.org/schema/richfaces/cdk/jsf/composite"
        >

    <cc:interface>
        <cdk:class>org.richfaces.renderkit.html.MenuSeparatorRenderer</cdk:class>
        <cdk:superclass>org.richfaces.ui.menu.MenuSeparatorRendererBase</cdk:superclass>
        <cdk:component-family>org.richfaces.DropDownMenu</cdk:component-family>
        <cdk:renderer-type>org.richfaces.MenuSeparatorRenderer</cdk:renderer-type>
        <cdk:renders-children>true</cdk:renders-children>
    </cc:interface>

    <cc:implementation>
        <cdk:object name="cssRoot" value="#{component.attributes['cssRoot']}" />
        <div class="rf-#{cssRoot}-sep"></div>
    </cc:implementation>

</cdk:root>
