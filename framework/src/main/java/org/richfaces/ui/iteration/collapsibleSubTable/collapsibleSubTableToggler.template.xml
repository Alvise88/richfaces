    <cc:interface>
        <cdk:class>org.richfaces.ui.iteration.collapsibleSubTable.CollapsibleSubTableTogglerRenderer</cdk:class>
        <cdk:superclass>org.richfaces.ui.iteration.collapsibleSubTable.CollapsibleSubTableTogglerRendererBase</cdk:superclass>
        <cdk:component-family>org.richfaces.CollapsibleSubTableToggler</cdk:component-family>
